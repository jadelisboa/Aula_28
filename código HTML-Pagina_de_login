    <script>
    function laranja () {
        $Usuario_f = this.LOGIN.fUsuario.value
        $Usuario = "Barril"
        $Senha = "Laranja"
        $Senha_f = this.LOGIN.fSenha.value
        if ($Usuario_f !== $Usuario){
            alert ("Usuário ou senha inválido")
            return false
        }
        if ($Senha_f !== $Senha){
            alert ("Usuário ou senha inválido")
            return false
        }
        if ($Usuario_f == $Usuario && $Senha_f == $Senha){
        document.write("<a href='Sites/Pagina1.html'> <center> <font size='7'> Entrar </font> </center> </a>")
        }
    }
</script></head>
